/**
 * 
 */
function runExample(){
	counterExample();
	
}

function counterExample(){
	
    function createCounter() {
        var numberOfCalls = 0;
        return function() {
           return ++numberOfCalls;
        }
     }
     var fn = createCounter();
     var text = '';
     text += fn() + ", "; //1
     text += fn() + ", "; //1, 2
     text += fn() + ", "; //1, 2, 3
}
