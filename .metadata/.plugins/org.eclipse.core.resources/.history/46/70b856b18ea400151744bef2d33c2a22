/**
 * 
 */
function Machine(){
	var enabled = false;
	
	this.enable = function(){
		enabled = true;
	};
	
	this.enable = function(){
		enabled = true;
	}
}

function CoffeeMachine(power){
	
	Machine.call(this);  // Inheritance
	
	var waterAmount = 0;
	
	this.setWaterAmount = function(amount){
		waterAmount = amount;
	}
}

function runCoffeeMachineExample(){
	
	var coffeeMachine = new  CoffeeMachine(400);
	
	
}
